<?xml version="1.0" encoding="utf-8"?>
<odoo>
  <data>
    <record id="module_category_ott_tc_base" model="ir.module.category">
      <field name="name">Ott TC Base</field>
      <field name="sequence">1</field>
    </record>
    <record id="group_ott_tc_base_user" model="res.groups">
      <field name="name">User</field>
      <field name="category_id" ref="ott_tc_base.module_category_ott_tc_base"/>
    </record>
    <record id="group_ott_tc_base_manager" model="res.groups">
      <field name="name">Manager</field>
      <field name="category_id" ref="ott_tc_base.module_category_ott_tc_base"/>
      <field name="implied_ids" eval="[(4, ref('group_ott_tc_base_user'))]"/>
      <field name="users" eval="[(4, ref('base.user_root')), (4, ref('base.user_admin'))]"/>
    </record>
  </data>
</odoo>
